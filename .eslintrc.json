{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "max-len": [
      "error",
      {
        "code": 90,
        "tabWidth": 2,
        "ignoreUrls": true
      }
    ]
  }
}
